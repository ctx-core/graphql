{
	"name": "@ctx-core/graphql",
	"version": "9.0.30",
	"description": "ctx-core graphql",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.23",
		"@ctx-core/function": "workspace:^20.2.3",
		"@ctx-core/object": "workspace:^21.0.22",
		"graphql": "^16.0.1",
		"nanographql": "^2.0.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.107",
		"@types/nanographql": "^2.0.1",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": []
}
