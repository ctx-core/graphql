{
	"name": "@ctx-core/graphql",
	"version": "9.1.124",
	"description": "ctx-core graphql",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./gql",
		"./graphql_validation_errors_",
		"./ValidationError"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^5.36.2",
		"graphql": "^16.8.1",
		"nanographql": "^2.0.0"
	},
	"devDependencies": {
		"@types/nanographql": "^2.0.3",
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [],
	"sideEffects": false
}