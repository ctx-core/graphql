{
	"name": "@ctx-core/graphql",
	"version": "8.0.20",
	"description": "ctx-core graphql",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^22.1.1",
		"@ctx-core/function": "workspace:^19.1.11",
		"@ctx-core/object": "workspace:^20.2.3",
		"graphql": "^15.5.1",
		"nanographql": "^2.0.0"
	},
	"devDependencies": {
		"@types/nanographql": "^2.0.1",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": []
}
